<?xml version='1.0' encoding='utf-8'?>
<widget id="com.elishconsulting.mnjic" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Gundoo's Mean</name>
    <description>
        Gundoo's mean mobile app
    </description>
    <author email="yadavsaloni2018@gmail.com" href="https://www.linkedin.com/in/saloni-yadav/">
        Frankenstein
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <access origin="https://gundoosmean-server.herokuapp.com;" />
    <allow-navigation href="*"/>
    <allow-intent href="https://gundoosmean-server.herokuapp.com;" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/img/gundoo192.png" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="mdpi" src="res/icon/android/gm_mdpi.png" />
        <icon density="hdpi" src="res/icon/android/gm_hdpi.png" />
        <icon density="xhdpi" src="res/icon/android/gm_xhdpi.png" />
        <icon density="xxhdpi" src="res/icon/android/gm_xxhdpi.png" />
        <icon density="xxxhdpi" src="res/icon/android/gm_xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>